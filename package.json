{
  "name": "ionic-app-base",
  "author": "Ionic",
  "homepage": "http://ionicframework.com/",
  "license": "MIT",
  "scripts": {
    "clean": "node ./config/build/clean.js",
    "compile-js": "node ./config/build/run-ngc.js",
    "rollup-app": "rollup -f iife -c ./config/rollup/rollup.config.app.js -o www/build/bundle.es6.js",
    "rollup-polyfills": "rollup -f iife -c ./config/rollup/rollup.config.polyfills.js -o www/build/polyfills.js",
    "rollup": "npm run rollup-app && npm run rollup-polyfills",
    "transpile-rollup": "tsc --out www/build/bundle.es5.js --target es5 --allowJs www/build/bundle.es6.js",
    "rename-bundles": "node ./config/build/rename-bundle.js ./www/build/bundle.es6.js ./www/build/bundle.es5.js ./www/build/bundle.js",
    "minify": "uglifyjs --screw-ie8 --compress --mangle --output ./www/build/bundle.min.js ./www/build/bundle.js",
    "gzip": "gzip -k ./www/build/bundle.min.js",
    "copy-html": "node ./config/build/copy-html.js",
    "build": "npm run clean && npm run compile-js && npm run rollup && npm run transpile-rollup && npm run rename-bundles && npm run minify && npm run gzip && npm run copy-html"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "core-js": "2.4.1",
    "ionic-angular": "nightly",
    "ionic-native": "1.3.20",
    "ionicons": "3.0.0",
    "rxjs": "5.0.0-beta.11",
    "zone.js": "0.6.17"
  },
  "devDependencies": {
    "@angular/compiler-cli": "0.6.0",
    "@angular/platform-server": "2.0.0-rc.6",
    "autoprefixer": "6.4.1",
    "compression-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "del": "2.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.2",
    "glob-all": "3.1.0",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.5",
    "html-webpack-plugin": "2.21.0",
    "ionic-gulp-tslint": "^1.1.0",
    "ionic-ngc-build": "0.0.1",
    "node-sass": "3.8.0",
    "postcss": "5.1.2",
    "resolve-url-loader": "^1.6.0",
    "rollup": "^0.34.13",
    "rollup-plugin-commonjs": "^4.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "run-sequence": "^1.1.5",
    "rxjs-es": "^5.0.0-beta.11",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-ionic-rules": "0.0.4",
    "typescript": "^2.0.2",
    "vinyl-fs": "2.4.3",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/driftyco/ionic.git"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
